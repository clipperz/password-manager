{
    "license": "AGPL-3.0-or-later",
    "scripts": {
        "build": "spago --jobs 10 build --purs-args '--codegen js,sourcemaps -o ./target/output.purescript'",
        "clean-purescript": "rm -rf target/output.purescript",
        "clean-spago": "rm -rf .spago",
        "clean-node": "rm -rf node_modules",
        "clean-webpack": "rm -rf target/output.webpack",
        "clean-test": "rm -rf target/output.purescript/test.js",
        "nuke": "npm run clean-node; npm run clean-spago; npm run clean-purescript; npm run clean-webpack",
        "docs": "spago docs --open",
        "package": "webpack build --config ./webpack.config.js",
        "keep-building": "spago --jobs 10 build --watch --purs-args '--codegen js,sourcemaps -o ./target/output.purescript'",
        "keep-packaging": "webpack watch --config ./webpack.config.js --stats verbose",
        "rebuild": "clear; npm run clean; npm run build",
        "test-browser": "(trap 'kill 0' SIGINT; webpack watch --config ./webpack.test.config.js & webpack serve --config ./webpack.test.config.js --stats verbose)",
        "upgrade-set": "spago upgrade-set"
    },
    "devDependencies": {
        "css-loader": "^6.7.3",
        "html-inline-script-webpack-plugin": "^3.1.0",
        "html-webpack-plugin": "^5.5.0",
        "process": "^0.11.10",
        "purescript": "0.15.15",
        "sass": "^1.49.0",
        "sass-loader": "^13.2.2",
        "spago": "0.21.0",
        "style-loader": "^3.3.2",
        "webpack": "^5.77.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.15.0"
    },
    "dependencies": {
        "big-integer": "^1.6.51",
        "buffer": "^6.0.3",
        "decimal.js": "^10.3.1",
        "downloadjs": "^1.4.7",
        "long": "4.0.0",
        "markdown-it": "^13.0.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "xhr2": "0.2.1"
    }
}
